<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <AssemblyName>OpenClawWinManager</AssemblyName>
    <RootNamespace>OpenClawWinManager</RootNamespace>
    <ApplicationTitle>OpenClawWinManager</ApplicationTitle>
    <Product>OpenClawWinManager</Product>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

</Project>
